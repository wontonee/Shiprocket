{
  "name": "wontonee/shiprocket",
  "description": "Shiprocket integration for Bagisto",
  "type": "bagisto-module",
  "license": "MIT",
  "authors": [
    {
      "name": "Saju Gopal",
      "email": "saju.g@wontonetech.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "bagisto/bagisto": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Wontonee\\Shiprocket\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wontonee\\Shiprocket\\Providers\\ShiprocketServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan vendor:publish --tag=public --force"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
